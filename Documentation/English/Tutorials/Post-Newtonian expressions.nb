 Cell[BoxData[
  RowBox[{"x", "\[Congruent]", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      FormBox[
       SubscriptBox["\[CapitalOmega]", "\[Phi]"],
       TraditionalForm], "M"}], ")"}], 
    RowBox[{"2", "/", "3"}]]}]],
  CellChangeTimes->{{3.741281025271906*^9, 3.741281067395288*^9}, {
   3.741281266708061*^9, 3.74128130599083*^9}},ExpressionUUID->
  "aed0c5e2-2c4a-4d74-94b6-cd55379dfef4"],
 "."
}], "Item",
 CellChangeTimes->{{3.741281025271906*^9, 3.741281067395288*^9}, {
  3.741281266708061*^9, 3.7412813087094517`*^9}, {3.7412822657470713`*^9, 
  3.7412822902998047`*^9}},ExpressionUUID->"c33152bc-a9fd-4663-a321-\
625204e76f93"],

Cell[TextData[{
 "Note that ",
 StyleBox["x",
  FontSlant->"Italic"],
 " is not available for Kerr black holes in BlackHoleAnalysis."
}], "Item",
 CellChangeTimes->{{3.741281025271906*^9, 3.741281067395288*^9}, {
  3.741281266708061*^9, 3.7412813087094517`*^9}, {3.7412822657470713`*^9, 
  3.741282315947095*^9}},ExpressionUUID->"ef14bf70-621a-4957-9fb1-\
e444ce363035"],

