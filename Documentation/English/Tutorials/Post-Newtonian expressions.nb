Cell[BoxData[
 InterpretationBox[
  RowBox[{
   FractionBox[
    RowBox[{"1", "-", 
     SuperscriptBox["e", "2"]}], "p"], "-", 
   FractionBox[
    RowBox[{"2", " ", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["e", "2"], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "1"}], "+", 
         SuperscriptBox["e", "2"]}], ")"}]}], ")"}]}], 
    SuperscriptBox["p", "2"]], "-", 
   FractionBox[
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", "1"}], "+", 
       SuperscriptBox["e", "2"]}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"10", " ", 
        SuperscriptBox["e", "4"]}], "-", 
       RowBox[{"10", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", 
          SqrtBox[
           RowBox[{"1", "-", 
            SuperscriptBox["e", "2"]}]]}], ")"}]}], "+", 
       RowBox[{
        SuperscriptBox["e", "2"], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "3"}], "+", 
          RowBox[{"10", " ", 
           SqrtBox[
            RowBox[{"1", "-", 
             SuperscriptBox["e", "2"]}]]}]}], ")"}]}]}], ")"}]}], 
    RowBox[{"2", " ", 
     SuperscriptBox["p", "3"]}]], "+", 
   InterpretationBox[
    SuperscriptBox[
     RowBox[{"O", "[", 
      FractionBox["1", "p"], "]"}], 
     RowBox[{"7", "/", "2"}]],
    SeriesData[$CellContext`p, 
     DirectedInfinity[1], {}, 2, 7, 2],
    Editable->False]}],
  SeriesData[$CellContext`p, 
   DirectedInfinity[1], {
   1 - $CellContext`e^2, 0, (-2) $CellContext`e^2 (-1 + $CellContext`e^2), 0, 
    Rational[-1, 2] (-1 + $CellContext`e^2) (10 $CellContext`e^4 - 
     10 (-1 + (1 - $CellContext`e^2)^Rational[1, 2]) + $CellContext`e^2 (-3 + 
       10 (1 - $CellContext`e^2)^Rational[1, 2]))}, 2, 7, 2],
  Editable->False]], "Output",
 CellLabel->"Out[6]=",ExpressionUUID->"b300695e-8a83-4341-aab9-e6b29fa0fe3f"]
}, Open  ]],

