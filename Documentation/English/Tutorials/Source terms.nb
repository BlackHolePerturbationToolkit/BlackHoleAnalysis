Cell[CellGroupData[{

Cell["Capital/lowercase source terms", \
"Subsection",ExpressionUUID->"91fe14e0-4423-4dbe-a312-1fb64111f0a4"],

Cell[CellGroupData[{

Cell[TextData[{
 "As introduced in Hopper & Evans (",
 ButtonBox["arxiv.org/abs/1006.4907",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://arxiv.org/abs/1006.4907"], None},
  ButtonNote->"https://arxiv.org/abs/1006.4907"],
 "), lowercase source terms have the Dirac delta \
\[OpenCurlyDoubleQuote]divided off\[CloseCurlyDoubleQuote]. That is,  ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    SubscriptBox["Q", "tt"], "[", 
    RowBox[{"t", ",", "r"}], "]"}], " ", "=", " ", 
   RowBox[{
    RowBox[{
     SubscriptBox["q", "tt"], "[", "t", "]"}], 
    RowBox[{"\[Delta]", "[", 
     RowBox[{"r", "-", 
      RowBox[{
       SubscriptBox["r", "p"], "[", "t", "]"}]}], "]"}]}]}]],ExpressionUUID->
  "907188be-e885-4b77-8c72-a7b4005b9959"]
}], "Item",ExpressionUUID->"ebdeed11-2803-4111-b61a-57e884e495f5"],

