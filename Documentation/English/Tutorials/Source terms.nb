Cell[CellGroupData[{

Cell["Capital/lowercase source terms", \
"Subsection",ExpressionUUID->"5de4c462-1591-478d-b289-17721b31c79c"],

Cell[CellGroupData[{

Cell[TextData[{
 "As introduced in Hopper & Evans (",
 ButtonBox["arxiv.org/abs/1006.4907",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://arxiv.org/abs/1006.4907"], None},
  ButtonNote->"https://arxiv.org/abs/1006.4907"],
 "), lowercase source terms have the Dirac delta \
\[OpenCurlyDoubleQuote]divided off\[CloseCurlyDoubleQuote]. That is,  ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    SubscriptBox["Q", "tt"], "[", 
    RowBox[{"t", ",", "r"}], "]"}], " ", "=", " ", 
   RowBox[{
    RowBox[{
     SubscriptBox["q", "tt"], "[", "t", "]"}], 
    RowBox[{"\[Delta]", "[", 
     RowBox[{"r", "-", 
      RowBox[{
       SubscriptBox["r", "p"], "[", "t", "]"}]}], "]"}]}]}]],ExpressionUUID->
  "b380baae-cb18-42df-9962-81ec657744d1"]
}], "Item",ExpressionUUID->"561f980e-de0c-46ee-942b-ccdb53caedd2"],

