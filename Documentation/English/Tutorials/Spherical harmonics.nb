Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"YHarmonic", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"l", ",", "m"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"\[Theta]", ",", "\[Phi]"}], "}"}]}], "]"}]], "Input",
 CellLabel->
  "(Local) In[3]:=",ExpressionUUID->"773cda4e-37d7-4c56-b252-7c410ff8b7ed"],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"\[ImaginaryI]", " ", "m", " ", "\[Phi]"}]], " ", 
   SqrtBox[
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"2", " ", "l"}]}], ")"}], " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"l", "-", "m"}], ")"}], "!"}]}], 
     RowBox[{
      RowBox[{"(", 
