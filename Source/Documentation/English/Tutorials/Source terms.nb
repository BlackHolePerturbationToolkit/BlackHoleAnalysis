Cell["Capital/lowercase source terms", "Subsection",ExpressionUUID->"4d99c87a-61b1-4657-b92b-b56d6deed514"],

Cell[TextData[{
 "As introduced in Hopper & Evans (",
 ButtonBox["arxiv.org/abs/1006.4907",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://arxiv.org/abs/1006.4907"], None},
  ButtonNote->"https://arxiv.org/abs/1006.4907"],
 "), lowercase source terms have the Dirac delta \
\[OpenCurlyDoubleQuote]divided off\[CloseCurlyDoubleQuote]. That is,  ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    SubscriptBox["Q", "tt"], "[", 
    RowBox[{"t", ",", "r"}], "]"}], " ", "=", " ", 
   RowBox[{
    RowBox[{
     SubscriptBox["q", "tt"], "[", "t", "]"}], 
    RowBox[{"\[Delta]", "[", 
     RowBox[{"r", "-", 
      RowBox[{
       SubscriptBox["r", "p"], "[", "t", "]"}]}], "]"}]}]}]],ExpressionUUID->
  "0624987f-7d2b-4f0e-b963-d898729a55e6"]
}], "Item",ExpressionUUID->"29d5ee6f-240b-4f01-9c04-eed0bdad64f7"],

